{
  "name": "discoid",
  "version": "0.1.6",
  "description": "A simple utility to retrieve disk info",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "windows-powershell": "^0.1.1"
  },
  "scripts": {
    "build": "babel src --out-dir dist --source-maps --copy-files",
    "release": "dotenv release-it",
    "example": "babel-node src/example/index.js",
    "prebuild": "rm -rf build && mkdir build",
    "postbuild": "",
    "report-coverage": "codecov",
    "test": "exit 0 # mocha ./src/lib/*.test.js --compilers js:babel-core/register",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "dotenv-cli": "^1.4.0",
    "ghooks": "^1.0.3",
    "istanbul": "^0.4.2",
    "mocha": "^2.3.4",
    "release-it": "^2.7.3",
    "semantic-release": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miniArray/discoid.git"
  },
  "keywords": [
    "disk",
    "windows",
    "system"
  ],
